{
  "$id": "option_chain_response.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OptionChainResponse",
  "type": "object",
  "required": ["output"],
  "properties": {
    "output": { "$ref": "#/definitions/OptionChainOutput" }
  },
  "definitions": {
    "OptionChainOutput": {
      "type": "object",
      "required": [
        "symbol",
        "daysToExpiration",
        "interestRate",
        "underlyingPrice",
        "volatility",
        "callExpDateMap",
        "putExpDateMap"
      ],
      "properties": {
        "symbol": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "underlying": { "$ref": "#/definitions/Underlying" },
        "strategy": {
          "type": "string",
          "enum": [
            "SINGLE",
            "ANALYTICAL",
            "COVERED",
            "VERTICAL",
            "CALENDAR",
            "STRANGLE",
            "STRADDLE",
            "BUTTERFLY",
            "CONDOR",
            "DIAGONAL",
            "COLLAR",
            "ROLL"
          ]
        },
        "interval": {
          "type": "string",
          "format": "decimal"
        },
        "isDelayed": {
          "type": "boolean"
        },
        "isIndex": {
          "type": "boolean"
        },
        "daysToExpiration": {
          "type": "string",
          "format": "decimal"
        },
        "interestRate": {
          "type": "string",
          "format": "decimal"
        },
        "underlyingPrice": {
          "type": "string",
          "format": "decimal"
        },
        "volatility": {
          "type": "string",
          "format": "decimal"
        },
        "callExpDateMap": {
          "type": "object",
          "additionalProperties": {
            "type": "object",
            "additionalProperties": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Option"
              }
            }
          }
        },
        "putExpDateMap": {
          "type": "object",
          "additionalProperties": {
            "type": "object",
            "additionalProperties": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Option"
              }
            }
          }
        }
      }
    },
    "Option": {
      "title": "Option",
      "type": "object",
      "properties": {
        "putCall": {
          "type": "string",
          "enum": ["PUT", "CALL"]
        },
        "symbol": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "exchangeName": {
          "type": "string"
        },
        "bidPrice": {
          "type": "string",
          "format": "decimal"
        },
        "askPrice": {
          "type": "string",
          "format": "decimal"
        },
        "lastPrice": {
          "type": "string",
          "format": "decimal"
        },
        "markPrice": {
          "type": "string",
          "format": "decimal"
        },
        "bidSize": {
          "type": "integer",
          "format": "int32"
        },
        "askSize": {
          "type": "integer",
          "format": "int32"
        },
        "lastSize": {
          "type": "integer",
          "format": "int32"
        },
        "highPrice": {
          "type": "string",
          "format": "decimal"
        },
        "lowPrice": {
          "type": "string",
          "format": "decimal"
        },
        "openPrice": {
          "type": "string",
          "format": "decimal"
        },
        "closePrice": {
          "type": "string",
          "format": "decimal"
        },
        "totalVolume": {
          "type": "integer",
          "format": "int32"
        },
        "quoteTimeInLong": {
          "type": "integer",
          "format": "int32"
        },
        "tradeTimeInLong": {
          "type": "integer",
          "format": "int32"
        },
        "netChange": {
          "type": "string",
          "format": "decimal"
        },
        "volatility": {
          "type": "string",
          "format": "decimal"
        },
        "delta": {
          "type": "string",
          "format": "decimal"
        },
        "gamma": {
          "type": "string",
          "format": "decimal"
        },
        "theta": {
          "type": "string",
          "format": "decimal"
        },
        "vega": {
          "type": "string",
          "format": "decimal"
        },
        "rho": {
          "type": "string",
          "format": "decimal"
        },
        "timeValue": {
          "type": "string",
          "format": "decimal"
        },
        "openInterest": {
          "type": "string",
          "format": "decimal"
        },
        "isInTheMoney": {
          "type": "boolean"
        },
        "theoreticalOptionValue": {
          "type": "string",
          "format": "decimal"
        },
        "theoreticalVolatility": {
          "type": "string",
          "format": "decimal"
        },
        "isMini": {
          "type": "boolean"
        },
        "isNonStandard": {
          "type": "boolean"
        },
        "optionDeliverablesList": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "symbol": {
                "type": "string"
              },
              "assetType": {
                "type": "string"
              },
              "deliverableUnits": {
                "type": "string"
              },
              "currencyType": {
                "type": "string"
              }
            }
          }
        },
        "strikePrice": {
          "type": "string",
          "format": "decimal"
        },
        "expirationDate": {
          "type": "string"
        },
        "expirationType": {
          "type": "string"
        },
        "multiplier": {
          "type": "string",
          "format": "decimal"
        },
        "settlementType": {
          "type": "string"
        },
        "deliverableNote": {
          "type": "string"
        },
        "isIndexOption": {
          "type": "boolean"
        },
        "percentChange": {
          "type": "string",
          "format": "decimal"
        },
        "markChange": {
          "type": "string",
          "format": "decimal"
        },
        "markPercentChange": {
          "type": "string",
          "format": "decimal"
        }
      }
    },
    "Underlying": {
      "title": "Underlying",
      "type": "object",
      "properties": {
        "ask": {
          "type": "string",
          "format": "decimal"
        },
        "askSize": {
          "type": "integer",
          "format": "int32"
        },
        "bid": {
          "type": "string",
          "format": "decimal"
        },
        "bidSize": {
          "type": "integer",
          "format": "int32"
        },
        "change": {
          "type": "string",
          "format": "decimal"
        },
        "close": {
          "type": "string",
          "format": "decimal"
        },
        "delayed": {
          "type": "boolean"
        },
        "description": {
          "type": "string"
        },
        "exchangeName": {
          "type": "string",
          "enum": ["IND", "ASE", "NYS", "NAS", "NAP", "PAC", "OPR", "BATS"]
        },
        "fiftyTwoWeekHigh": {
          "type": "string",
          "format": "decimal"
        },
        "fiftyTwoWeekLow": {
          "type": "string",
          "format": "decimal"
        },
        "highPrice": {
          "type": "string",
          "format": "decimal"
        },
        "last": {
          "type": "string",
          "format": "decimal"
        },
        "lowPrice": {
          "type": "string",
          "format": "decimal"
        },
        "mark": {
          "type": "string",
          "format": "decimal"
        },
        "markChange": {
          "type": "string",
          "format": "decimal"
        },
        "markPercentChange": {
          "type": "string",
          "format": "decimal"
        },
        "openPrice": {
          "type": "string",
          "format": "decimal"
        },
        "percentChange": {
          "type": "string",
          "format": "decimal"
        },
        "quoteTime": {
          "type": "integer",
          "format": "int64"
        },
        "symbol": {
          "type": "string"
        },
        "totalVolume": {
          "type": "integer",
          "format": "int64"
        },
        "tradeTime": {
          "type": "integer",
          "format": "int64"
        }
      }
    },
    "ExpirationDate": {
      "title": "ExpirationDate",
      "type": "object",
      "properties": {
        "date": {
          "type": "string"
        }
      }
    },
    "OptionDeliverables": {
      "title": "OptionDeliverables",
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string"
        },
        "assetType": {
          "type": "string"
        },
        "deliverableUnits": {
          "type": "string"
        },
        "currencyType": {
          "type": "string"
        }
      }
    }
  }
}

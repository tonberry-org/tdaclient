{
  "$id": "get_account_request.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GetAccountRequest",
  "type": "object",
  "required": ["authorization", "request"],
  "properties": {
    "authorization": {
      "$ref": "authorization.json#/definitions/Authorization"
    },
    "request": {
      "type": "object",
      "required": ["accountId"],
      "properties": {
        "fields": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["positions", "orders"]
          }
        },
        "accountId": {
          "type": "string"
        }
      }
    }
  }
}
